{
  "manifest_version": 2,
  "name": "Gimme Word",
  "description": "Easy to use word lookup and synonym finder",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content/content.js"]
    }
  ],
  "background": {
    "scripts": ["background/background.js"]
  }
}
